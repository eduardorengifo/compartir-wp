{
  "name": "oscar254/compartir-wp",
  "description": "Plugin to share new content through social networks automatically.",
  "keywords": [
    "share",
    "bot",
    "facebook"
  ],
  "type": "wordpress-plugin",
  "license": "GPL2",
  "require": {
    "php": ">=5.6",
    "facebook/graph-sdk": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*"
  },
  "autoload": {
    "psr-4": {
      "CompartirWP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}